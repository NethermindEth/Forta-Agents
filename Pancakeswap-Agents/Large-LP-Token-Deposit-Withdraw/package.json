{
  "name": "pancakeswap-large-lptoken-deposit-withdraw",
  "version": "0.0.1",
  "description": "Detects Large LP Token Deposits or Withdrawals on Pancakeswap",
  "repository": "https://github.com/NethermindEth/Forta-Agents/tree/main/PancakeSwap-Bots/Large-LP-Token-Deposit-Withdraw",
  "chainIds": [
    56
  ],
  "scripts": {
    "build": "tsc",
    "start": "npm run start:dev",
    "start:dev": "nodemon --watch src --watch forta.config.json -e js,ts,json  --exec \"npm run build && forta-agent run\"",
    "start:prod": "forta-agent run --prod",
    "tx": "npm run build && forta-agent run --tx",
    "block": "npm run build && forta-agent run --block",
    "range": "npm run build && forta-agent run --range",
    "file": "npm run build && forta-agent run --file",
    "publish": "forta-agent publish",
    "push": "forta-agent push",
    "disable": "forta-agent disable",
    "enable": "forta-agent enable",
    "keyfile": "forta-agent keyfile",
    "test": "jest --detectOpenHandles",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "forta-agent": "^0.1.8",
    "lru-cache": "^7.13.1"
  },
  "devDependencies": {
    "@types/nodemon": "^1.19.0",
    "bignumber.js": "^9.0.1",
    "forta-agent-tools": "^2.2.1",
    "jest": "^27.0.6",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.4"
  }
}
